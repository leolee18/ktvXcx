.son-list-one{
  position: relative;
  width: 100%;
  height: 104rpx;
  border-bottom: 1rpx solid #eff0eb;
  background-color: #fff;
  padding: 20rpx 40rpx;
  box-sizing: border-box;
  font-size: 35rpx;
  line-height: 64rpx;
}


.son-list-k{
    position: absolute;
    width: 64rpx;
    height: 64rpx;
    top: 20rpx;
    right: 30rpx;
    z-index: 3;
    overflow: hidden;
    pointer-events: none;
}
.son-list-k::after{
    content:"";
    position: absolute;
    width: 30%;
    height: 30%;
    top: 35%;
    left: 35%;
    transform: rotate(45deg);
    border-right: 4rpx solid #b5b5b5;
    border-top: 4rpx solid #b5b5b5;
}



.sonc-list-one{
  position: relative;
  width: 100%;
  height: 123rpx;
  border-bottom: 1rpx solid #eff0eb;
  padding: 20rpx 30rpx;
  box-sizing: border-box;
  font-size: 35rpx;
  line-height: 64rpx;
}
.sonc-list-so-name{
  position: relative;
  display: block;
  font-size: 30rpx;
  line-height: 45rpx;
  color: #282828;
}
.sonc-list-si-name{
  position: relative;
  display: block;
  font-size: 24rpx;
  line-height: 40rpx;
  color: #8a8a8a;
}
.sonc-list-but{
  position: absolute;
  width: 114rpx;
  height: 73rpx;
  top: 20rpx;
  right: 30rpx;
  border-radius: 40rpx;
  border: 1rpx solid #b4c4be;
  font-size: 30rpx;
  color: #6fb3c5;
  line-height: 73rpx;
  text-align: center;
}